{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "max-line-length": false,
        "object-literal-sort-keys": false,
        "max-classes-per-file": false,
        "arrow-parens": false,
        "curly": false,
        "completed-docs": [true, "classes", "methods", "functions", "interfaces", "enums"]
    }
}